<h1>Welcome To U-Save Liquor Mart!</h1>
<p>Your One stop shop that covers all of your beverage needs!</p>
<p>Choose from our wide ranged selection! </p>
 




<% if (allrecs.length > 0) {%>
  <% allrecs.forEach((recref, index) => { %>
    <product>
      <table> 
      <tr>
        <td><%= recref.productname %></td>
        <td><%= recref.saleprice %></td>
      </tr>
      <tr>
        <td><%= recref.productimage %></td>
        <td><%= recref.productstatus %></td>
      </tr>
      <tr>
        <td><%= recref.startingcity %></td>
        <td><%= recref.destinationcity %></td>
      </tr>
      </table>
          </product>
      <% }); %>
<% } %>

      
    
       
  

  
  




